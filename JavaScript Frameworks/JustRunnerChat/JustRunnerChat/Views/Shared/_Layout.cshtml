<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    @*<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />*@
  <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
  <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
   <script src="~/ChatScripts/pubnub.min.js"></script>
</head>
<body>
    <header>
        <div class="content-wrapper clear-fix">
            <div class="float-left">
                <p class="site-title">
                    <a href="~/">Just Runner Chat</a>
                </p>
            </div>
            <div class="float-right">
                <nav>
                    <ul id="menu">
                        <!--<li>@Html.ActionLink("Home", "Index", "Home", new { area = "" }, null)</li>-->
                        <li><a href="#" id="go-register">Register</a></li>
                        <li><a href="#" id="go-login">Login</a></li>
                        <li>@Html.ActionLink("API", "Index", "Help", new { area = "" }, null)</li>
                    </ul>
                </nav>
            </div>
        </div>
    </header>
    
    
    @RenderBody()

    
    @RenderSection("scripts", required: false)
@*    @Scripts.Render("~/Scripts/js")*@
     
    <script src="../../Scripts/class.js"></script>
    <script src="../../Scripts/q.js"></script>
    <script src="../../Scripts/sha1.js"></script>
    <script src=http://cdn.pubnub.com/pubnub-3.3.min.js></script>
    @Scripts.Render("~/ChatScripts/js")
</body>
</html>
